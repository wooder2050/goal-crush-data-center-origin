{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}",
    "src/app/**/page.tsx",
    "src/app/**/layout.tsx",
    "src/app/**/route.ts"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["**/*.stories.tsx", "**/*.test.ts", "**/*.test.tsx"],
  "ignoreDependencies": [],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": [],
  "rules": {
    "exports": "off",
    "types": "off"
  }
}
